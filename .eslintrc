{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "globals": {
    "styled": true
  },
  "env": {
    "es6": true,
    "serviceworker": true,
    "browser": true,
    "commonjs": true,
    "node": true,
    "mocha": true
  },
  "rules": {
    "arrow-parens": 0,
    "quotes": [1, "single"],
    "eqeqeq": 1,
    "max-len": 0,
    "new-cap": 0,
    "no-unused-vars": 1,
    "no-bitwise": 0,
    "comma-dangle": 0,
    "spaced-comment": 0,
    "padded-blocks": 1,
    "space-in-parens": 0,
    "space-before-function-paren": 0,
    "no-underscore-dangle": 0,
    "default-case": 0,
    "import/extensions": 0,
    "import/no-unresolved": 0,
    "newline-per-chained-call": 0,
    "react/prefer-stateless-function": 0,
    "react/prefer-es6-class": 1,
    "react/prop-types": 1,
    "react/jsx-filename-extension": 0,
    "react/jsx-boolean-value": 0,
    "react/forbid-prop-types": 0,
    "react/no-danger": 0,
    "jsx-a11y/label-has-for": 0,
    "jsx-a11y/no-static-element-interactions": 0
  }
}
