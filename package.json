{
  "name": "soluml-react-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for React projects.",
  "private": true,
  "main": "index.js",
  "scripts": {
    "clean": "rimraf .build",
    "prebuild-dev": "npm run clean",
    "build-dev": "BABEL_ENV=client webpack",
    "prebuild": "npm run clean",
    "build": "BABEL_ENV=client webpack -p",
    "pretest": "npm run typecheck",
    "test": "echo \"Error: no test specified\" && exit 1",
    "typecheck": "tsc",
    "prestart": "npm run clean",
    "start": "BABEL_ENV=client webpack-dev-server",
    "prepush": "npm test"
  },
